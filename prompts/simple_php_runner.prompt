@context(../context/php_runner.instruction.prompt)

class Factory.Car
{
  private int year
  private string make

  public constructor(int year, string make) {
    this.year = year
    this.make = make
    print "this is a new in {year} {make}"
  }

  public function hello() {
    print "hello from factory vehicle {this.year} {this.make}"
  }
}

let car = new Car()
car.hello()
