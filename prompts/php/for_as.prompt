@context(../../context/python_script_runner.instruction.prompt)

let final_word = (run cli command: ./agent prompts/php/helper/get_happy_new_year_greets.prompt)

for final_word {
  prepend "This is the message from the helper, for 2026: "
}

let md5hash = (run cli command: ./agent prompts/php/helper/md5_for_test.prompt)

print {final_word} . ":" . {md5hash}
